{
  "name": "aoba-meal-app",
  "version": "2.1.0",
  "private": true,
  "homepage": "https://mokemoke0821.github.io/aoba-meal-app",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/lab": "^7.0.0-beta.13",
    "@mui/material": "^7.1.2",
    "@mui/x-data-grid": "^8.5.3",
    "@mui/x-date-pickers": "^8.5.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.19.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.58.1",
    "react-scripts": "5.0.1",
    "react-to-print": "^3.1.0",
    "recharts": "^2.15.4",
    "typescript": "^4.9.5",
    "uuid": "^11.1.0",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "scripts": {
    "start": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" craco start",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" DISABLE_ESLINT_PLUGIN=true GENERATE_SOURCEMAP=false PUBLIC_URL=/aoba-meal-app craco build",
    "build:dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" craco build",
    "build:pwa": "npm run build && npm run test:pwa",
    "test": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" craco test --testTimeout=60000 --transformIgnorePatterns=\"node_modules/(?!(date-fns|@mui|@emotion)/)\"",
    "test:ci": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" craco test --watchAll=false --testTimeout=60000 --coverage",
    "test:pwa": "node scripts/test-pwa.js",
    "validate:manifest": "node -e \"console.log(JSON.parse(require('fs').readFileSync('public/manifest.json', 'utf-8')))\" && echo 'manifest.json is valid'",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@mui/icons-material": "^7.1.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^30.0.0",
    "@types/testing-library__user-event": "^4.1.1",
    "babel-jest": "^29.7.0",
    "craco-workbox": "^0.2.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.3.0",
    "identity-obj-proxy": "^3.0.0",
    "resolve-url-loader": "^5.0.0",
    "ts-jest": "^29.4.0",
    "vite-plugin-pwa": "^1.0.3",
    "web-vitals": "^5.0.3",
    "webpack-cli": "^6.0.1",
    "workbox-window": "^7.3.0"
  }
}